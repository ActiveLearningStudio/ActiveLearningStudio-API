{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^2.10",
        "gnello/laravel-mattermost-driver": "^1.2",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": "^7.0.1",
        "h5p/h5p-core": "^1.24",
        "h5p/h5p-editor": "^1.24",
        "kaltura/api-client-library": "^17.5",
        "laravel/framework": "^9.0",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "maatwebsite/excel": "^3.1",
        "madnest/madzipper": "^1.1",
        "opigno/tincan": "^1.1"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fzaninotto/faker": "^1.9.1",
        "knuckleswtf/scribe": "^3.37",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Djoudi\\LaravelH5p\\LaravelH5pServiceProvider"
            ],
            "aliases": {
                "LaravelH5p": "Djoudi\\LaravelH5p\\Facades"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Djoudi\\LaravelH5p\\": "H5P/laravel-h5p/src/LaravelH5p",
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "app/Services/CurrikiGo"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Djoudi\\LaravelH5p\\": "H5P/laravel-h5p/src/LaravelH5p",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
