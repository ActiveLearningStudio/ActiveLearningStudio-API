{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "babenkoivan/elastic-migrations": "^1.2",
        "babenkoivan/elastic-scout-driver": "^1.2",
        "babenkoivan/elastic-scout-driver-plus": "^1.7",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "gnello/laravel-mattermost-driver": "^1.2",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": "^6.3",
        "h5p/h5p-core": "^1.24",
        "h5p/h5p-editor": "^1.24",
        "kaltura/api-client-library": "^17.5",
        "laravel/framework": "^7.0",
        "laravel/passport": "^9.3",
        "laravel/scout": "^8.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "maatwebsite/excel": "^3.1",
        "madnest/madzipper": "^1.1",
        "predis/predis": "^1.1",
        "rusticisoftware/tincan": "@stable",
        "superbalist/laravel-prometheus-exporter": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.4.2",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "mpociot/laravel-apidoc-generator": "^4.8",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Djoudi\\LaravelH5p\\LaravelH5pServiceProvider"
            ],
            "aliases": {
                "LaravelH5p": "Djoudi\\LaravelH5p\\Facades"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Djoudi\\LaravelH5p\\": "H5P/laravel-h5p/src/LaravelH5p",
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Services/CurrikiGo"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Djoudi\\LaravelH5p\\": "H5P/laravel-h5p/src/LaravelH5p",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
